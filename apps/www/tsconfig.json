{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"esModuleInterop": true,
		"strict": true,
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "node",
		"jsxImportSource": "solid-js",
		"jsx": "preserve",
		"types": ["solid-start/env", "solid-jsx/types"],
		"baseUrl": "./",
		"paths": {
			"@/*": ["./src/*"]
		}
	}
}

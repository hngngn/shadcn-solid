  "files": [
    {
      "path": "src/examples/tabs-demo.tsx",
      "content": "import { Button } from \"@repo/tailwindcss/ui/v4/button\"\nimport {\n  Card,\n  CardContent,\n  CardDescription,\n  CardFooter,\n  CardHeader,\n  CardTitle,\n} from \"@repo/tailwindcss/ui/v4/card\"\nimport {\n  Tabs,\n  TabsContent,\n  TabsIndicator,\n  TabsList,\n  TabsTrigger,\n} from \"@repo/tailwindcss/ui/v4/tabs\"\nimport {\n  TextField,\n  TextFieldInput,\n  TextFieldLabel,\n} from \"@repo/tailwindcss/ui/v4/text-field\"\n\nconst TabsDemo = () => {\n  return (\n    <div class=\"flex w-full max-w-sm flex-col gap-6\">\n      <Tabs defaultValue=\"account\">\n        <TabsList>\n          <TabsTrigger value=\"account\">Account</TabsTrigger>\n          <TabsTrigger value=\"password\">Password</TabsTrigger>\n          <TabsIndicator />\n        </TabsList>\n        <TabsContent value=\"account\">\n          <Card>\n            <CardHeader>\n              <CardTitle>Account</CardTitle>\n              <CardDescription>\n                Make changes to your account here. Click save when you&apos;re\n                done.\n              </CardDescription>\n            </CardHeader>\n            <CardContent class=\"grid gap-6\">\n              <TextField defaultValue=\"Pedro Duarte\" class=\"grid gap-3\">\n                <TextFieldLabel>Name</TextFieldLabel>\n                <TextFieldInput />\n              </TextField>\n              <TextField defaultValue=\"@peduarte\" class=\"grid gap-3\">\n                <TextFieldLabel>Username</TextFieldLabel>\n                <TextFieldInput />\n              </TextField>\n            </CardContent>\n            <CardFooter>\n              <Button>Save changes</Button>\n            </CardFooter>\n          </Card>\n        </TabsContent>\n        <TabsContent value=\"password\">\n          <Card>\n            <CardHeader>\n              <CardTitle>Password</CardTitle>\n              <CardDescription>\n                Change your password here. After saving, you&apos;ll be logged\n                out.\n              </CardDescription>\n            </CardHeader>\n            <CardContent class=\"grid gap-6\">\n              <TextField defaultValue=\"\" class=\"grid gap-3\">\n                <TextFieldLabel>Current password</TextFieldLabel>\n                <TextFieldInput />\n              </TextField>\n              <TextField defaultValue=\"\" class=\"grid gap-3\">\n                <TextFieldLabel>New password</TextFieldLabel>\n                <TextFieldInput />\n              </TextField>\n            </CardContent>\n            <CardFooter>\n              <Button>Save password</Button>\n            </CardFooter>\n          </Card>\n        </TabsContent>\n      </Tabs>\n    </div>\n  )\n}\n\nexport default TabsDemo\n",
      "type": "registry:example"
    }
  ]
}
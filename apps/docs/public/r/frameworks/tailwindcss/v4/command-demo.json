      "path": "src/examples/command-demo.tsx",
      "content": "import type { Accessor, JSX } from \"solid-js\"\nimport { For } from \"solid-js\"\n\nimport {\n  Command,\n  CommandEmpty,\n  CommandGroup,\n  CommandInput,\n  CommandItem,\n  CommandList,\n  CommandShortcut,\n} from \"@repo/tailwindcss/ui/v4/command\"\n\ninterface Option {\n  icon: Accessor<JSX.Element>\n  label: string\n  disabled: boolean\n  shortcut?: Accessor<JSX.Element>\n}\n\ninterface List {\n  label: string\n  options: Option[]\n}\n\nconst commandData: List[] = [\n  {\n    label: \"Suggestions\",\n    options: [\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            viewBox=\"0 0 24 24\"\n            class=\"mr-2 h-4 w-4\"\n          >\n            <path\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n              d=\"M4 7a2 2 0 0 1 2-2h12a2 2 0 0 1 2 2v12a2 2 0 0 1-2 2H6a2 2 0 0 1-2-2zm12-4v4M8 3v4m-4 4h16m-9 4h1m0 0v3\"\n            />\n          </svg>\n        ),\n        label: \"Calendar\",\n        disabled: false,\n      },\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"mr-2 h-4 w-4\"\n            viewBox=\"0 0 24 24\"\n          >\n            <g\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n            >\n              <path d=\"M3 12a9 9 0 1 0 18 0a9 9 0 1 0-18 0m6-2h.01M15 10h.01\" />\n              <path d=\"M9.5 15a3.5 3.5 0 0 0 5 0\" />\n            </g>\n          </svg>\n        ),\n        label: \"Search emoji\",\n        disabled: false,\n      },\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"mr-2 h-4 w-4\"\n            viewBox=\"0 0 24 24\"\n          >\n            <g\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n            >\n              <path d=\"M4 13a8 8 0 0 1 7 7a6 6 0 0 0 3-5a9 9 0 0 0 6-8a3 3 0 0 0-3-3a9 9 0 0 0-8 6a6 6 0 0 0-5 3\" />\n              <path d=\"M7 14a6 6 0 0 0-3 6a6 6 0 0 0 6-3m4-8a1 1 0 1 0 2 0a1 1 0 1 0-2 0\" />\n            </g>\n          </svg>\n        ),\n        label: \"Launch\",\n        disabled: false,\n      },\n    ],\n  },\n  {\n    label: \"Settings\",\n    options: [\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"mr-2 h-4 w-4\"\n            viewBox=\"0 0 24 24\"\n          >\n            <path\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n              d=\"M8 7a4 4 0 1 0 8 0a4 4 0 0 0-8 0M6 21v-2a4 4 0 0 1 4-4h4a4 4 0 0 1 4 4v2\"\n            />\n          </svg>\n        ),\n        label: \"Profile\",\n        disabled: true,\n        shortcut: () => <CommandShortcut>⌘P</CommandShortcut>,\n      },\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"mr-2 h-4 w-4\"\n            viewBox=\"0 0 24 24\"\n          >\n            <g\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n            >\n              <path d=\"M3 7a2 2 0 0 1 2-2h14a2 2 0 0 1 2 2v10a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2z\" />\n              <path d=\"m3 7l9 6l9-6\" />\n            </g>\n          </svg>\n        ),\n        label: \"Mail\",\n        disabled: false,\n        shortcut: () => <CommandShortcut>⌘B</CommandShortcut>,\n      },\n      {\n        icon: () => (\n          <svg\n            xmlns=\"http://www.w3.org/2000/svg\"\n            class=\"mr-2 h-4 w-4\"\n            viewBox=\"0 0 24 24\"\n          >\n            <g\n              fill=\"none\"\n              stroke=\"currentColor\"\n              stroke-linecap=\"round\"\n              stroke-linejoin=\"round\"\n              stroke-width=\"2\"\n            >\n              <path d=\"M10.325 4.317c.426-1.756 2.924-1.756 3.35 0a1.724 1.724 0 0 0 2.573 1.066c1.543-.94 3.31.826 2.37 2.37a1.724 1.724 0 0 0 1.065 2.572c1.756.426 1.756 2.924 0 3.35a1.724 1.724 0 0 0-1.066 2.573c.94 1.543-.826 3.31-2.37 2.37a1.724 1.724 0 0 0-2.572 1.065c-.426 1.756-2.924 1.756-3.35 0a1.724 1.724 0 0 0-2.573-1.066c-1.543.94-3.31-.826-2.37-2.37a1.724 1.724 0 0 0-1.065-2.572c-1.756-.426-1.756-2.924 0-3.35a1.724 1.724 0 0 0 1.066-2.573c-.94-1.543.826-3.31 2.37-2.37c1 .608 2.296.07 2.572-1.065\" />\n              <path d=\"M9 12a3 3 0 1 0 6 0a3 3 0 0 0-6 0\" />\n            </g>\n          </svg>\n        ),\n        label: \"Setting\",\n        disabled: false,\n        shortcut: () => <CommandShortcut>⌘S</CommandShortcut>,\n      },\n    ],\n  },\n]\n\nconst CommandDemo = () => {\n  return (\n    <Command class=\"border\">\n      <CommandInput placeholder=\"Type a command or search...\" />\n      <CommandList>\n        <CommandEmpty>No results found.</CommandEmpty>\n        <For each={commandData}>\n          {(item) => (\n            <CommandGroup heading={item.label}>\n              <For each={item.options}>\n                {(item) => (\n                  <CommandItem disabled={item.disabled}>\n                    {item.icon()}\n                    <span>{item.label}</span>\n                    {item.shortcut?.()}\n                  </CommandItem>\n                )}\n              </For>\n            </CommandGroup>\n          )}\n        </For>\n      </CommandList>\n    </Command>\n  )\n}\n\nexport default CommandDemo\n",
      "type": "registry:example"
    }
  ]
}